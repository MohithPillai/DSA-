int middle(int A, int B, int C){
        //code here//Position this line where user code will be pasted.
        if(A<B && A<C)
        {
            if(B<C) return B;
            return C;
        }
        else if(B<A && B<C)
        {
            if(A<C) return A;
            else return C;
        }
        else
        {
            if(B<A) return B;
            else return A;
        }
    }
